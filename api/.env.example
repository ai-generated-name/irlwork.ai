# irlwork.ai API Configuration
# Copy this file to .env and fill in your values

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Server
PORT=3002
NODE_ENV=development

# CORS
CORS_ORIGINS=https://www.irlwork.ai,http://localhost:5173

# Cloudflare R2 (for proof uploads)
R2_CLOUDFLARE_ACCOUNT_ID=your-account-id
R2_ACCESS_KEY_ID=your-access-key
R2_SECRET_ACCESS_KEY=your-secret-key
R2_BUCKET_NAME=irlwork-proofs

# Wallet/Payments (Base network)
PLATFORM_WALLET_ADDRESS=0x...
PLATFORM_WALLET_PRIVATE_KEY=0x...
PLATFORM_FEE_PERCENT=10

# API
API_URL=http://localhost:3002
