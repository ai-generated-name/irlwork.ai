{
  "buildCommand": "cd ui && npm install --legacy-peer-deps && npm run build",
  "outputDirectory": "ui/dist",
  "installCommand": "echo 'skip'",
  "framework": null,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
